node('maven'){
    stage('Checkout'){
        echo "Cloning the Repo by deepak"
        git credentialsId: 'git_token_generated', url: 'https://github.com/deepaklama0815/jenkins-practice.git'
    }
    
    stage('runningScript'){
        echo "Running the Script by deepak"
        sh "sh test.sh"
    }
    
    stage('Final'){
        echo "The job is completed successfully ya-hoo"
    }
}
